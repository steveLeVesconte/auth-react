.players {

  display: flex;
  gap: 1rem;

  flex-direction: row;
  grid-area: players;
  background-color: rgb(130, 215, 238);
  height: 100%;
  width: 100%;
  outline: 1px black solid;
}
.player-box{
  flex-grow: 1;
}
.player{
  flex-grow: 1;  
  background-color: rgb(0, 149, 255);
  outline: 1px black solid;
}

.opponent{
  flex-grow: 1;
  background-color: rgb(128, 0, 0);
  outline: 1px black solid;
}
.player-stone {
  height: 20px;
  width: 20px;
  min-width: 20px;
}

.actions {
  grid-area: actions;
  background-color: aqua;
  width: 100%;
  outline: 1px black solid;

}
.chat {
  grid-area: chat;
  background-color: thistle;
  min-height: 250px;
  width: 100%;
  outline: 1px black solid;
}

