/* board intersections  */

.boardIntersectionWithStone {
  position: relative;
  top: 0;
  left: 0;
  display: flex;
}
.emptyIntersection {
  position: relative;
  top: 0;
  left: 0;
  display: flex;
}

.stone {
  position: absolute;
  top: 0px;
  left: 0px;
  height: 90%;
  width: 90%;
}

.pending-svg {
  position: absolute;
  top: 0px;
  left: 0px;
  height: 70%;
  width: 70%;
  margin: 14%;
}
.last-turn-svg {
  position: absolute;
  top: 0px;
  left: 0px;
  height: 70%;
  width: 70%;
  margin: 14%;
}

div.intersection-hover:hover {
  opacity: 0.8 !important;
  background-color: rgb(184, 190, 175);
  cursor: pointer;
  border-radius: 50px;
}

.svgBack {
  height: 100%;
  width: 100%;
}


